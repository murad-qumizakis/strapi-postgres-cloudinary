      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "link": {
      "type": "text",
      "required": true,
      "unique": false
    },
    "sub_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-category.sub-category",
      "inversedBy": "videos"
    }
  }
}
